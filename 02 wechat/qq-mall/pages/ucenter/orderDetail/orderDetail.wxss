.orderBox1 {
  width: 92%;
  height: 84rpx;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.statusText {
  font-size: 28rpx;
  color: #666;
}

.orderStatus {
  font-size: 28rpx;
  color: #333;
  text-align: right;
}

.countDown {
  padding-top: 15rpx;
  height: 84rpx;
  display: flex;
  flex-direction: column;
}

.countTime {
  font-size: 20rpx;
  color: #666;
}

.orderBox2 {
  width: 100%;
  display: flex;
  flex-direction: column;
}

.commoditys {
  width: 100%;
  height: 208rpx;
  position: relative;
}

.commoditys image {
  width: 144rpx;
  height: 146rpx;
  position: absolute;
  top: 30rpx;
  left: 30rpx;
}

.name {
  font-size: 26rpx;
  color: #333;
  position: absolute;
  top: 32rpx;
  left: 200rpx;
}

.price {
  font-size: 34rpx;
  font-weight: bold;
  color: #FA0201;
  position: absolute;
  top: 130rpx;
  left: 199rpx;
}

.number {
  font-size: 28rpx;
  color: #999;
  position: absolute;
  top: 133rpx;
  right: 30rpx;
}

.priceBox {
  width: 100%;
  height: 84rpx;
  border-top: 1rpx #f8f8f8 solid;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.payText {
  font-size: 28rpx;
  color: #666;
  margin-left: 30rpx;
}

.prices {
  font-size: 30rpx;
  color: #FA0201;
  margin-right: 30rpx;
}

.orderBox3 {
  width: 100%;
  height: 126rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.addressBox {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-left: 42rpx;
}

.namePhone {
  width: 100%;
  font-size: 28rpx;
  color: #333;
  text-align: left;
}

.addressDetail {
  width: 100%;
  font-size: 26rpx;
  color: #666;
  text-align: left;
}

.orderBox3 image {
  width: 14rpx;
  height: 24rpx;
  margin-right: 30rpx;
}

.orderBox4 {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
  margin-bottom: 98rpx;
}

.orderBox4 .caption {
  margin: 30rpx 30rpx 10rpx 30rpx;
  font-size: 28rpx;
  color: #666;
}

.orderBox4 text {
  margin: 5rpx 30rpx 5rpx 30rpx;
  font-size: 26rpx;
  color: #999;
}
.orderBox4 .infos {
  display: flex;
  flex-direction: column;
  margin: 10rpx 0;
}

.orderBox5 {
  width: 100%;
  height: 98rpx;
  border-top: 1rpx #f8f8f8 solid;
  position: fixed;
  bottom: 0;
  background: #fff;
}

.btn1 {
  height: 52rpx;
  line-height: 52rpx;
  text-align: center;
  border-radius: 4rpx;
  font-size: 28rpx;
  color: #666;
  position: absolute;
  top: 24rpx;
  right: 212rpx;
}

.btn2 {
  height: 52rpx;
  line-height: 52rpx;
  text-align: center;
  border-radius: 4rpx;
  font-size: 28rpx;
  color: #fff;
  background: #FF5B34;
  position: absolute;
  top: 24rpx;
  right: 32rpx;
}

.btn3 {
  height: 52rpx;
  line-height: 52rpx;
  text-align: center;
  border-radius: 4rpx;
  font-size: 28rpx;
  color: #666;
  position: absolute;
  top: 24rpx;
  right: 32rpx;
}
.canvas-box {
  height: 200px;
  width: 300px;
}
.canvas-box canvas{
  height: 200px;
  margin-left: 18%;
}
.cs{
  margin-bottom: 25%;
  padding-bottom: 40px;
}
