page{
  background: #f6f6f6;
}

.navBox {
  width: 100%;
  height: 90rpx;
  display: flex;
  justify-content: space-between;
  background: #fff;
  border-top: 1rpx #f6f6f6 solid;
}

.nav {
  width: 20%;
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 32rpx;
  color: #333;
}

.active {
  color: #FF5B34;
  position: relative;
}

.active::after {
  content: "";
  width: 93rpx;
  height: 4rpx;
  background: #FF6541;
  position: absolute;
  bottom: 0;
  left: 50%;
  margin-left: -46rpx;
}

.orders {
  height: auto;
  width: 100%;
  overflow: hidden;
}

.order {
  margin-top: 20rpx;
  background: #fff;
}

.order .h {
  height: 84rpx;
  line-height: 84rpx;
  padding: 0 30rpx;
  font-size: 28rpx;
  color: #999;
  display: flex;
  justify-content: space-between;
}

.order .goods {
  width: 100%;
  height: 179rpx;
  position: relative;
  border-top: 1rpx #E7E7E7 solid;
  border-bottom: 1rpx #E7E7E7 solid;
}

.order .goods image {
  width: 144rpx;
  height: 146rpx;
  position: absolute;
  top: 15rpx;
  left: 30rpx;
}

.order .goods .name {
  width: 53.333333%;
  height: 68rpx;
  color: #333;
  font-size: 26rpx;
  position: absolute;
  top: 18rpx;
  left: 200rpx;
}

.order .goods .price {
  width: 53.333333%;
  color: #FA0201;
  font-size: 34rpx;
  font-weight: bold;
  position: absolute;
  bottom: 42rpx;
  left: 199rpx;
}

.order .goods .number {
  color: #999;
  font-size: 28rpx;
  position: absolute;
  bottom: 42rpx;
  right: 30rpx;
}

.order .b {
  height: 107rpx;
  line-height: 107rpx;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.order .b .l {
  font-size: 28rpx;
  color: #666;
}

.order .b .r {
  height: 50rpx;
  display: flex;
  justify-content: space-between;
}

.order .b .btn1 {
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  border-radius: 4rpx;
  font-size: 26rpx;
  color: #999;
}

.order .b .btn2 {
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  border-radius: 4rpx;
  font-size: 26rpx;
  color: #fff;
  background: #FF5B34;
  margin-left: 40rpx;
}

.order .b .btn3 {
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  border-radius: 4rpx;
  font-size: 26rpx;
  color: #999;
}

.loadmore {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  margin-top: 0rpx;
}

.loadmore text {
  color: #999;
}

@keyframes loading {
  0% {transform: rotate(0deg)}
  50% {transform: rotate(180deg)}
  100% {transform: rotate(360deg)}
}

.loading {
  display: inline-block;
  transform-origin: 50% 50%;
  animation: loading 1s linear infinite;
}

